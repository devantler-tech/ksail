{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    "group:dotnetMonorepo",
    ":disableDependencyDashboard",
    ":semanticCommitTypeAll(fix)"
  ],
  "github-actions": {
    "enabled": false
  },
  "minor": {
    "automerge": true
  },
  "patch": {
    "automerge": true
  }
}
